{
  "name": "@asso/api",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/main.ts",
    "build": "tsc -p tsconfig.json",
    "pretest": "prisma generate",
    "test": "vitest run --coverage",
    "lint": "echo \"Lint is managed at the repository root\"",
    "format": "echo \"Format is managed at the repository root\"",
    "start": "node dist/main.js",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "jobs:worker": "tsx watch --clear-screen=false src/jobs/worker.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@fastify/autoload": "6.3.1",
    "@fastify/env": "5.0.2",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "10.3.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@prisma/instrumentation": "6.16.2",
    "@sentry/node": "^10.15.0",
    "@prisma/client": "6.16.2",
    "argon2": "^0.44.0",
    "bullmq": "^5.58.9",
    "clamdjs": "1.0.2",
    "html-to-text": "9.0.5",
    "mjml": "^4.16.1",
    "nodemailer": "^7.0.6",
    "dotenv": "17.2.2",
    "fastify": "^5.6.1",
    "fastify-plugin": "5.0.1",
    "jsonwebtoken": "9.0.2",
    "pdf-lib": "1.17.1",
    "pino": "9.11.0",
    "prom-client": "15.1.3",
    "redis": "5.8.2",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mjml": "^4.7.4",
    "@types/nodemailer": "^7.0.1",
    "@types/node": "^24.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "esbuild-register": "3.6.0",
    "pg": "^8.16.3",
    "prisma": "6.16.2",
    "supertest": "^7.1.4",
    "ts-node": "10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.2",
    "vitest": "^3.2.4",
    "redis-memory-server": "^0.13.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
